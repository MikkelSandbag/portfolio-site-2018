{
  "name": "mikkel-portfolio-2018",
  "version": "2.0.0",
	"private": true,
	"homepage": "https://mikkelsandberg.com",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "html-react-parser": "^0.4.6",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-image-lightbox": "^5.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-ga": "^1.0.0",
    "react-scripts": "1.1.4",
    "react-sizeme": "^2.5.2",
    "react-stack-grid": "^0.7.1",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/SassImports src/ -o src/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/SassImports src/ -o src/ --output-style compressed --watch --recursive --usePolling",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
