{
  "name": "mikkel-sandberg-portfolio-2018",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "html-react-parser": "^0.4.6",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-image-lightbox": "^5.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-stack-grid": "^0.7.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/SassImports src/ -o src/ --output-style expanded",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/SassImports src/ -o src/ --output-style expanded --watch --recursive --usePolling",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5"
  }
}
